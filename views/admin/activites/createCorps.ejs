<div class="grid-start">
    <div class=" quest adminForm2">
        <form action="/admin/activites/createCorps" method="POST">
            <label>Titre
                <input type="text" name="titre" required>
            </label>
            <label>Description
                <textarea type="textarea" class="champText" name="description" required></textarea>
            </label>
            <label>Sous-categorie
                <select name="sousCategorie">
                    <option value="puissance">Puissance</option>
                    <option value="technique">Technique</option>
                    <option value="souplesse">Souplesse</option>
                    <option value="sante">Santé</option>
                </select>
            </label>
            <label>Difficulté
                <input type="text" name="difficulte" required>
            </label>
            <label>%Puissance
                <input type="text" name="pourcentagePuissance" required>
            </label>
            <label>%Technique
                <input type="text" name="pourcentageTechnique" required>
            </label>
            <label>%Souplesse
                <input type="text" name="pourcentageSouplesse" required>
            </label>
            <label>%Sante
                <input type="text" name="pourcentageSante" required>
            </label>
            <label>Prérequis F
                <input type="text" name="prerequisF" required>
            </label>
            <label>Prérequis D
                <input type="text" name="prerequisD" required>
            </label>
            <label>Prérequis C
                <input type="text" name="prerequisC" required>
            </label>
            <label>Prérequis B
                <input type="text" name="prerequisB" required>
            </label>
            <label>Prérequis A
                <input type="text" name="prerequisA" required>
            </label>
            <label>Prérequis S
                <input type="text" name="prerequisS" required>
            </label>
            <input id="buttonCreate" type="submit" value="CREER">
        </form>
    </div>
</div>