<div class= "grid-hero">
    <div class="new1">
        <h1>Go !</h1>
        <h3>Choisis une catégorie pour ta nouvelle activité</h3>

        <div class="">
            <h3><%= hero.social.aspirationSocial %></h3>   <!--aspirationSocial.titre-->
            <form class="formNewActivities" action=" <%= `/users/newActivities/${hero._id}/social` %>" method="POST">
                <h5><%= hero.social.scoreSocial %></h5> <!--user.social.scorePuissance-->
                <select name="sousCategorie">
                    <option value="potes">Potes</option>
                    <option value="aventure">Aventure</option>
                    <option value="charisma">Charisma</option>
                </select>
                <input type="submit" value="NEW" class="buttonNew">
            </form>
        </div>
    </div>
</div>